{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"args": [
				"--release"
			],
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "rust: cargo build"
		},
		{
			"type": "cargo",
			"command": "run",
			"args": [
				"--release",
				"--package",
				"aoc2022",
				"--bin",
				"aoc2022",
			],
			"problemMatcher": [
				"$rustc"
			],
			"label": "rust: run aoc2022"
		}
	]
}